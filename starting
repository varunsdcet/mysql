brew install mysql@5.7
cd ~/
touch .bash_profile
open .bash_profile
export PATH="/usr/local/opt/mysql@5.7/bin:$PATH"
then save
mysql -uroot -p
mysql_secure_installation
mysql -u root -p
show databases;
CREATE DATABASE varun;
use varun;
SHOW table;
$(brew --prefix mysql)/bin/mysqladmin -u root password NEWPASS;
